{
  "root": true,
  "env": {
    "es6": true,
    "node": true,
    "react-native/react-native": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "plugins": ["prettier", "react", "react-native"],
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": "off"
  },
  "settings": {
    "react": {
      "version": "detect" 
    }
  }
}
